<?xml version="1.0" encoding="utf-8"?>
<hexya>
    <data>
	<view id="view_change_production_qty_wizard" model="ChangeProductionQty" >
                <form string="Change Product Qty">
                    <group>
                        <field name="product_qty"/>
                        <field name="mo_id" invisible="1"/>
                    </group>
                    <footer>
                        <button name="change_prod_qty" string="Approve"
                            colspan="1" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel" />
                    </footer>
                </form>
            </view>
	<action id="action_change_production_qty" type="ir.actions.act_window" name="Change Quantity To Produce" model="ChangeProductionQty" view_mode="form" >
    </data>
</hexya>
